filebeat.inputs:
- type: log
  enabled: true
  paths:
    - /usr/local/tomcat/logs/*.log

  multiline.pattern: '^[0-9]{4}-[0-9]{2}-[0-9]{2}'
  multiline.negate: true
  multiline.match: after

# ------------------------------ Logstash Output -------------------------------
output.logstash:
  # The Logstash hosts
  hosts: ["{{ elastic4waf }}:5044"]

